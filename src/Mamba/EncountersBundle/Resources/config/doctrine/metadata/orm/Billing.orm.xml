<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Billing" table="Billing">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="appId" type="integer" column="app_id"/>
    <field name="userId" type="integer" column="user_id"/>
    <field name="operationId" type="integer" column="operation_id"/>
    <field name="amount" type="float" column="amount"/>
    <field name="amountDeveloper" type="float" column="amount_developer"/>
    <field name="validationId" type="integer" column="validation_id"/>
    <field name="time" type="integer" column="time"/>
    <field name="sig" type="string" column="sig" length="64"/>
    <field name="billed" type="boolean" column="billed"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
